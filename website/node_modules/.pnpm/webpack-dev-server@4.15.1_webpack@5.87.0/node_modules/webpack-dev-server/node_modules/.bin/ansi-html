#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html" "$@"
else
  exec node  "$basedir/../../../../../ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html" "$@"
fi
