#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/html-minifier-terser@6.1.0/node_modules/html-minifier-terser/node_modules:/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/html-minifier-terser@6.1.0/node_modules:/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/html-minifier-terser@6.1.0/node_modules/html-minifier-terser/node_modules:/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/html-minifier-terser@6.1.0/node_modules:/home/krug/Coding/Python/Pathfinding-Algorithms/website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
